"*":
  "MavensMate-Atom":
    mm_close_panel_on_successful_operation: false
    mm_panel_height: 134
  "Sublime-Style-Column-Selection":
    mouseButtonTrigger: "Middle"
    selectKeyTrigger: "None"
  "atom-typescript":
    preferredQuoteCharacter: "'"
  "autoclose-html":
    forceInline: [
      "*"
    ]
  core:
    disabledPackages: [
      "less-than-slash"
      "linter-htmlhint"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "node_modules"
      "vendor"
      "*-meta.xml"
    ]
    packagesWithKeymapsDisabled: [
      "atom-transpose"
    ]
    projectHome: "C:\\workspace"
    telemetryConsent: "no"
    themes: [
      "atom-dark-ui-slim"
      "monokai"
    ]
  editor:
    fontSize: 12
    invisibles:
      tab: "â€”"
    preferredLineLength: 120
    showInvisibles: true
    tabLength: 4
  "exception-reporting":
    userId: "65c01e40-120a-c16d-be4b-1b5325c70803"
  "file-icons":
    strategies:
      usertypes: false
    tabPaneIcon: false
  linter:
    errorPanelHeight: 22
    lintOnFlyInterval: 1000
  "linter-htmlhint": {}
  "seti-icons":
    noColor: true
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
".apex.source":
  editor:
    showIndentGuide: true
